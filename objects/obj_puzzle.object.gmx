            <string>if (mode==M_SMASH) {
    smash_timer--;
    if (smash_timer&lt;0) {
        scr_smash();
    }
}
if (mode==M_CASCADE) {
    smash_timer--;
    if (smash_timer&lt;0) {
        scr_cascade();
    }
}
if (mode==M_ATK_COMBO) {
    smash_timer--;
    if (smash_timer&lt;0) {
        scr_attack_begin_animation();
    }
}
if (mode==M_ATK_RISE) {
    smash_timer--;
    if (smash_timer&lt;0) {
        scr_attack_end();
    }
}
if (mode==M_ATK_HIT) {
    smash_timer--;
    if (smash_timer&lt;0) {
        scr_mon_attack_begin();
    }
}
if (mode==M_MATK) {
    smash_timer--;
    scr_mon_attack_update()
}
